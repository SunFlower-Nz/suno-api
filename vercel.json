{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "functions": {
    "src/app/api/**/*.ts": {
      "runtime": "vercel-nodejs@4.0.0",
      "maxDuration": 60
    }
  },
  "env": {
    "BROWSER": "chromium",
    "BROWSER_HEADLESS": "true",
    "BROWSER_LOCALE": "en-US",
    "BROWSER_GHOST_CURSOR": "true"
  }
}
